{
  "expo": {
    "name": "REM",
    "slug": "rem",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/rem-icon.png",
    "scheme": "rem",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a1a"
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#0a0a1a"
    },
    "android": {
      "package": "com.erayoz.rem",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/rem-icon.png",
        "backgroundColor": "#0a0a1a"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "backgroundColor": "#0a0a1a"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0f6b78b5-5880-47db-af6c-1d3c3d83be1e"
      }
    }
  }
}
